{
  "name": "btc-simulator",
  "version": "0.0.1",
  "main": "btcsimulator/server/public/javascripts/app.js",
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components"
  ],
  "dependencies": {
    "backbone": "1.1.2",
    "chaplin": "1.0.0",
    "console-polyfill": "0.1.0",
    "lodash": "2.4.1",
    "jquery": "2.0.2",
    "normalize-css": "2.1.2",
    "h5bp-helpers": "0.1.0",
    "momentjs": "2.4.0",
    "backbone-pageable": "1.4.1",
    "backbone-relational": "0.8.7",
    "backbone.stickit": "0.6.3",
    "flat-ui": "2.1.1",
    "jquery.transit": "~0.9.9",
    "i18next": "1.7.1",
    "nprogress": "0.1.2",
    "sugar": "1.4.1",
    "d3": "~3.4.8",
    "nvd3": "~1.1.15-beta"
  },
  "overrides": {
    "backbone": {
      "dependencies": {
        "lodash": "*",
        "jquery": "*"
      },
      "main": "backbone.js"
    },
    "backbone-pageable": {
      "main": "lib/backbone-pageable.js"
    },
    "backbone-relational": {
      "main": "backbone-relational.js"
    },
    "backbone.stickit": {
      "main": "backbone.stickit.js"
    },
    "jquery-file-upload": {
      "main": [
        "css/jquery.fileupload.css",
        "css/jquery.fileupload-ui.css",
        "js/vendor/jquery.ui.widget.js",
        "js/jquery.iframe-transport.js",
        "js/jquery.fileupload.js",
        "js/jquery.fileupload-process.js",
        "js/jquery.fileupload-image.js"
      ]
    },
    "flat-ui": {
      "main": [
        "js/jquery-ui-1.10.3.custom.min.js",
        "js/jquery.ui.touch-punch.min.js",
        "js/bootstrap.min.js",
        "js/bootstrap-select.js",
        "js/bootstrap-switch.js",
        "js/flatui-checkbox.js",
        "js/flatui-radio.js",
        "js/jquery.tagsinput.js",
        "js/jquery.placeholder.js",
        "js/jquery.stackable.js"
      ]
    },
    "jquery.transit": {
      "main": "jquery.transit.js"
    }
  }
}
